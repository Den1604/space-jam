    Call  MakeEntity    ( .Entity:HiddenYellowBlock ~Vec4d:HiddenBlock2 .Item:ApplePie 80000000 )
    Call  AssignBlockFlag   ( *MF_btf_15_HiddenBlock2 )
    Return
    End
}

